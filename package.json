{
  "name": "hltvapi",
  "private": true,
  "scripts": {
    "build:prod": "npx webpack build --config ./webpack.prod.js",
    "build:dev": "npx webpack build --config ./webpack.dev.js",
    "build": "npm run build:prod",
    "serve": "npx webpack serve --config ./webpack.dev.js",
    "watch": "npx webpack build --config ./webpack.dev.js --watch",
    "stats:prod": "npx webpack build --config ./webpack.prod.js --profile --json > stats.json",
    "stats:dev": "npx webpack build --config ./webpack.dev.js --profile --json > stats.json",
    "stats": "npm run stats:prod",
    "clean:prod": "npx rimraf ./web/dist",
    "clean": "npm run clean:prod",
    "lint": "npm run lint:styles && npm run lint:html && npm run lint:js",
    "lint:js": "npx eslint --ext .js",
    "lint:html": "npx eslint --ext .html",
    "lint:styles": "npx stylelint ./web/*.css",
    "cypress:open": "npx cypress open"
  },
  "engines": {
    "npm": ">=7",
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f4bio/hltvapi.rs.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "author": {
    "name": "Fabio Tea",
    "email": "iam@f4b.io",
    "url": "https://iam.f4b.io"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/f4bio/hltvapi.rs/issues"
  },
  "homepage": "https://github.com/f4bio/hltvapi.rs#readme",
  "dependencies": {
    "alpinejs": "^3.4.2",
    "loglevel": "^1.7.1",
    "redoc": "^2.0.0-rc.56",
    "tailwindcss": "^2.2.16"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@fontsource/fira-sans": "^4.5.0",
    "@iconify/iconify": "^2.0.4",
    "@tailwindcss/forms": "^0.3.4",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "html-loader": "^2.1.2",
    "html-minimizer-webpack-plugin": "^3.3.0",
    "html-webpack-plugin": "^5.3.2",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.0",
    "mini-css-extract-plugin": "^2.4.2",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.1.1",
    "postcss-modules": "^4.2.2",
    "postcss-preset-env": "^6.7.0",
    "posthtml": "^0.16.5",
    "posthtml-expressions": "^1.9.0",
    "posthtml-iconify": "^0.2.1",
